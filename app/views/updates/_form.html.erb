<%= simple_form_for(@update), url: user_project_updates_path do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :picture %>
    <%= f.input :string %>
    <%= f.input :update_date %>
    <%= f.input :date %>
    <%= f.input :hours %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
